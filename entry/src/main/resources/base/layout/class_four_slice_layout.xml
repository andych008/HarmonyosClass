<?xml version="1.0" encoding="utf-8"?>
<!-- 第5节课布局-->
<ScrollView xmlns:ohos="http://schemas.huawei.com/res/harmonyos"
            ohos:width="match_parent"
            ohos:height="match_parent"
            ohos:background_element="#ffffff">
    <DirectionalLayout
            ohos:width="match_parent"
            ohos:height="match_content"
            ohos:orientation="vertical">
        <Button
                ohos:id="$+id:rdb_created"
                ohos:width="100vp"
                ohos:height="30vp"
                ohos:background_element="#ffeeeeee"
                ohos:text="创建关系型数据库"
                ohos:left_margin="5vp"
                ohos:right_margin="5vp"
                ohos:bottom_margin="5vp"
                ohos:top_margin="5vp"/>

        <Button
                ohos:id="$+id:rdb_insert"
                ohos:width="100vp"
                ohos:height="30vp"
                ohos:background_element="#ffeeeeee"
                ohos:text="关系型数据库增加"
                ohos:left_margin="5vp"
                ohos:right_margin="5vp"
                ohos:bottom_margin="5vp"
                ohos:top_margin="5vp"/>

        <Button
                ohos:id="$+id:rdb_delete"
                ohos:width="100vp"
                ohos:height="30vp"
                ohos:background_element="#ffeeeeee"
                ohos:text="关系型数据库删除"
                ohos:left_margin="5vp"
                ohos:right_margin="5vp"
                ohos:bottom_margin="5vp"
                ohos:top_margin="5vp"/>

        <Button
                ohos:id="$+id:rdb_updata"
                ohos:width="100vp"
                ohos:height="30vp"
                ohos:background_element="#ffeeeeee"
                ohos:text="关系型数据库修改"
                ohos:left_margin="5vp"
                ohos:right_margin="5vp"
                ohos:bottom_margin="5vp"
                ohos:top_margin="5vp"/>

        <Button
                ohos:id="$+id:rdb_query"
                ohos:width="100vp"
                ohos:height="30vp"
                ohos:background_element="#ffeeeeee"
                ohos:text="关系型数据库查询"
                ohos:left_margin="5vp"
                ohos:right_margin="5vp"
                ohos:bottom_margin="5vp"
                ohos:top_margin="5vp"/>
        <!-- 对象关系型数据库-->
        <Button
                ohos:id="$+id:orm_created"
                ohos:width="100vp"
                ohos:height="30vp"
                ohos:background_element="#ffeeeeee"
                ohos:text="创建对象关系型数据库"
                ohos:left_margin="5vp"
                ohos:right_margin="5vp"
                ohos:bottom_margin="5vp"
                ohos:top_margin="5vp"/>

        <Button
                ohos:id="$+id:orm_insert"
                ohos:width="100vp"
                ohos:height="30vp"
                ohos:background_element="#ffeeeeee"
                ohos:text="对象关系型数据库增加"
                ohos:left_margin="5vp"
                ohos:right_margin="5vp"
                ohos:bottom_margin="5vp"
                ohos:top_margin="5vp"/>

        <Button
                ohos:id="$+id:orm_delete"
                ohos:width="100vp"
                ohos:height="30vp"
                ohos:background_element="#ffeeeeee"
                ohos:text="对象关系型数据库删除"
                ohos:left_margin="5vp"
                ohos:right_margin="5vp"
                ohos:bottom_margin="5vp"
                ohos:top_margin="5vp"/>

        <Button
                ohos:id="$+id:orm_updata"
                ohos:width="100vp"
                ohos:height="30vp"
                ohos:background_element="#ffeeeeee"
                ohos:text="对象关系型数据库修改"
                ohos:left_margin="5vp"
                ohos:right_margin="5vp"
                ohos:bottom_margin="5vp"
                ohos:top_margin="5vp"/>

        <Button
                ohos:id="$+id:orm_query"
                ohos:width="100vp"
                ohos:height="30vp"
                ohos:background_element="#ffeeeeee"
                ohos:text="对象关系型数据库查询"
                ohos:left_margin="5vp"
                ohos:right_margin="5vp"
                ohos:bottom_margin="5vp"
                ohos:top_margin="5vp"/>
    </DirectionalLayout>
</ScrollView>